<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AndreaPerezSemana7.Registro">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="txtNombre" Placeholder="Ingrese Nombre"/>
            <Entry x:Name="txtUsuario" Placeholder="Ingrese usuario" Keyboard="Email"/>
            <Entry x:Name="txtContrasenia" Placeholder="Ingrese contrasenia" IsPassword="True"/>
            <Button x:Name="btnAgregar" Text="Agregar" Clicked="btnAgregar_Clicked" BackgroundColor="Azure"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>